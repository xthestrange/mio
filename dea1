var uniforms = {
  home: {
    angle: 0,
    textcolor: 0x1400CF,
    color1: 0x000000,
    color2: 0x000000,
    color3: 0x000000
  },
  guest: {
    angle: 60,
    textcolor: 0xF2D3B3,
    color1: 0xED8470,
    color2: 0xC76F5F,
    color3: 0xA15A4C
  }
};

room.setTeamColors(Team.RED, uniforms.home.angle, uniforms.home.textcolor, [uniforms.home.color1, uniforms.home.color2, uniforms.home.color3]);
room.setTeamColors(Team.BLUE, uniforms.guest.angle, uniforms.guest.textcolor, [uniforms.guest.color1, uniforms.guest.color2, uniforms.guest.color3]);

/* OBJECTS */

function Goal(time, team, striker, assist) {
	this.time = time;
	this.team = team;
	this.striker = striker;
	this.assist = assist;
}

function Game(date, scores, goals) {
	this.date = date;
	this.scores = scores;
	this.goals = goals;
}

/* FUNCTIONS */

/* AUXILIARY FUNCTIONS */

function getRandomInt(max) { // returns a random number from 0 to max-1
	return Math.floor(Math.random() * Math.floor(max)); 
}

function getTime(scores) { // returns the current time of the game
	return "[" + Math.floor(Math.floor(scores.time/60)/10).toString() + Math.floor(Math.floor(scores.time/60)%10).toString() + ":" + Math.floor(Math.floor(scores.time - (Math.floor(scores.time/60) * 60))/10).toString() + Math.floor(Math.floor(scores.time - (Math.floor(scores.time/60) * 60))%10).toString() + "]"
}

function getRandomPhrase() {
  var randomIndex = Math.floor(Math.random() * cr7Phrases.length);
  return cr7Phrases[randomIndex];
}

function canUseCr7(player) {
    var currentTime = new Date().getTime(); // Obtener el tiempo actual en milisegundos
    if (!(player.id in cr7Cooldowns)) {
        return true; // Permitir el uso si es la primera vez que el jugador usa el comando
    }
    var lastCr7Time = cr7Cooldowns[player.id]; // Obtener el √∫ltimo tiempo en que se us√≥ el comando por el jugador
    return (currentTime - lastCr7Time >= cr7Cooldown);
}

function pointDistance(p1, p2) {
	var d1 = p1.x - p2.x;
	var d2 = p1.y - p2.y;
	return Math.sqrt(d1 * d1 + d2 * d2);
}
function useTulaCommand(player) {
  if (!canUseTulaCommand[player.id]) {
    const randomSize = Math.floor(Math.random() * 25) + 1;
    const playerName = player.name;
    const responseMessage = `[üçå] La tula de ${playerName} mide ${randomSize} cm!`;
    room.sendAnnouncement(responseMessage, null, 0xF2F752, "bold");

    canUseTulaCommand[player.id] = true;
        setTimeout(() => {
            canUseTulaCommand[player.id] = false;
        }, 20000); // Bloquea el comando durante 20 segundos para el jugador espec√≠fico
    } else {
        room.sendAnnouncement("[üçå]¬°Espera un momento antes de volver a usar el comando Tula!", player.id, 0xFF7900, "bold");
    }
}

